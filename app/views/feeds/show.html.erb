<%= render 'base' %>
<div class="tools">
  <%= link_to '修改', edit_feed_path(@feed), :class => "btn btn-small btn-success" %>
  <%= link_to '返回', feeds_path, :class => "btn btn-small" %>
</div>
<div id="feed">
    <p>
    <b>Created at:</b>
    <%= @feed.created_at %>
  </p>
    <p>
    <b>Text:</b>
    <%= @feed.text %>
  </p>
    <p>
    <b>User:</b>
    <%= @feed.user %>
  </p>
  </div>